<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="./src/main/resources/mybatis/mysql-connector-java-5.1.25.jar"/>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://111.229.78.80:3306/mashup?useSSL=false"
                        userId="root"
                        password="wangzherongyao">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.xrikui.mashup.entity"
                            targetProject="../mashup/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapper"
                         targetProject="../mashup/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.xrikui.mashup.mapper"
                             targetProject="../mashup/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

       <!-- <table schema="" tableName="t_stock_config" domainObjectName="StockConfig"></table>-->
        <table schema="" tableName="t_mashup_config" domainObjectName="MashupConfig"></table>

    </context>
</generatorConfiguration>